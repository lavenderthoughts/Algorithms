#include<stdio.h>
#include<stdlib.h>
#include<string.h>

void swap(char *a, char *b) 
{
    char temp = *a;
    *a = *b;
    *b = temp;
}

int main()
{
	int i,j;
	char str[]="Heuristic";
	int len=strlen(str);
	for(i=0;i<=len-2;i++)
	{
		int min=i;
		for(j=i+1;j<=len-1;j++)
		{
			if(str[min]>str[j])
			{
				min=j;
			}
		}
		swap(&str[min],&str[i]);
	}
	i=0;
	while(str[i]!='\0')
	{
		printf("%c",str[i]);
		i++;
	}
}
